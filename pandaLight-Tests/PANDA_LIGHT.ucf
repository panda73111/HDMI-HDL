
CONFIG VCCAUX  = 3.3;

# Constraint for RX
NET "rx_channels_in[3]" TNM_NET = "RX_CLOCK";
TIMESPEC TS_RX_CLOCK = PERIOD "RX_CLOCK" 74 MHz HIGH 50 %;

NET "clk20" PERIOD = 20 MHz;

# RX clock
NET "RX_CHANNELS_IN_P[3]" IOSTANDARD = TMDS_33;
NET "RX_CHANNELS_IN_N[3]" IOSTANDARD = TMDS_33;
# RX red
NET "RX_CHANNELS_IN_P[2]" IOSTANDARD = TMDS_33;
NET "RX_CHANNELS_IN_N[2]" IOSTANDARD = TMDS_33;
# RX green
NET "RX_CHANNELS_IN_P[1]" IOSTANDARD = TMDS_33;
NET "RX_CHANNELS_IN_N[1]" IOSTANDARD = TMDS_33;
# RX blue
NET "RX_CHANNELS_IN_P[0]" IOSTANDARD = TMDS_33;
NET "RX_CHANNELS_IN_N[0]" IOSTANDARD = TMDS_33;

# TX clock
NET "TX_CHANNELS_OUT_P[3]" IOSTANDARD = TMDS_33;
NET "TX_CHANNELS_OUT_N[3]" IOSTANDARD = TMDS_33;
# TX red
NET "TX_CHANNELS_OUT_P[2]" IOSTANDARD = TMDS_33;
NET "TX_CHANNELS_OUT_N[2]" IOSTANDARD = TMDS_33;
# TX green
NET "TX_CHANNELS_OUT_P[1]" IOSTANDARD = TMDS_33;
NET "TX_CHANNELS_OUT_N[1]" IOSTANDARD = TMDS_33;
# TX blue
NET "TX_CHANNELS_OUT_P[0]" IOSTANDARD = TMDS_33;
NET "TX_CHANNELS_OUT_N[0]" IOSTANDARD = TMDS_33;

NET "BT_CTS" IOSTANDARD = LVCMOS33;
NET "BT_RTS" IOSTANDARD = LVCMOS33;
NET "BT_RXD" IOSTANDARD = LVCMOS33;
NET "BT_TXD" IOSTANDARD = LVCMOS33;
NET "CLK20" IOSTANDARD = LVCMOS33;
NET "LEDS_CLK" IOSTANDARD = LVCMOS33;
NET "LEDS_DATA" IOSTANDARD = LVCMOS33;
NET "RX_CEC" IOSTANDARD = LVCMOS33;
NET "RX_DET" IOSTANDARD = LVCMOS33;
NET "RX_EN" IOSTANDARD = LVCMOS33;
NET "TX_CEC" IOSTANDARD = LVCMOS33;
NET "TX_EN" IOSTANDARD = LVCMOS33;
NET "USB_CTS" IOSTANDARD = LVCMOS33;
NET "USB_RTS" IOSTANDARD = LVCMOS33;
