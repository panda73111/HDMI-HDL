NET "USB_RIN"  LOC = P82 | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;

# Bluetooth
NET "BT_CTS"  LOC = P6 | IOSTANDARD = LVCMOS33;
NET "BT_RTS"  LOC = P9 | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
