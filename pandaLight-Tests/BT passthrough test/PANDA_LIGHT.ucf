# USB UART
NET "USB_CTSN" LOC = P93 | IOSTANDARD = LVCMOS33;
NET "USB_RTSN" LOC = P92 | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
NET "USB_RXD"  LOC = P95 | IOSTANDARD = LVCMOS33;
NET "USB_TXD"  LOC = P94 | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
NET "USB_DTRN" LOC = P84 | IOSTANDARD = LVCMOS33 | SLEW = FAST | DRIVE = 8;
